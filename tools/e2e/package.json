{
  "name": "cypres-example-e2e",
  "version": "0.0.0",
  "description": "e2e test for your project",
  "main": "index.js",
  "scripts": {
    "cypress": "cypress",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:record": "cypress --record --key 5d50b928-8cea-4934-a135-29e045686be2",
    "cypress:report": "npx cypress run --browser electron  --reporter mocha-allure-reporter",
    "report:allure": "allure generate allure-results --clean -o allure-report && allure open  allure-report",
    "test:allure": "npm run cypress:report && npm run report:allure",
    "cy:run": "npx cypress run --browser chrome",
    "cy:parallel": "cypress-parallel -s cy:run -t 4 -m false",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rdwentmp/cypress-example"
  },
